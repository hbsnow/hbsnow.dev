{
  "name": "hbsnow.dev",
  "version": "1.0.0",
  "main": "index.js",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "NODE_ENV=development rm -rf .next && next build && next export",
    "start": "NODE_ENV=production next start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint 'src/**/*.{ts,tsx}' && prettier --check --ignore-path .gitignore '**/*.{js,json,md}' && secretlint '**/*'",
    "lintfix": "eslint --fix 'src/**/*.{ts,tsx}' && prettier --write --ignore-path .gitignore '**/*.{js,json,md}'",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "contentful-typescript-codegen": "npx contentful-typescript-codegen --output src/models/contentful.d.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "secretlint"
    ],
    "*.{js,json,md}": [
      "prettier --write --ignore-path .gitignore '**/*.{js,json,md}'",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "2.2.6",
    "color": "3.1.2",
    "dayjs": "1.8.23",
    "normalize.css": "8.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-markdown": "4.3.1",
    "react-test-renderer": "16.13.1",
    "react-use": "13.27.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@secretlint/secretlint-rule-preset-recommend": "1.1.0",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-knobs": "5.3.18",
    "@storybook/addon-storysource": "5.3.18",
    "@storybook/core": "5.3.18",
    "@storybook/react": "5.3.18",
    "@storybook/source-loader": "5.3.18",
    "@testing-library/jest-dom": "5.3.0",
    "@testing-library/react": "10.0.2",
    "@testing-library/react-hooks": "3.2.1",
    "@types/color": "3.0.1",
    "@types/jest": "25.2.1",
    "@types/next": "9.0.0",
    "@types/node": "13.11.0",
    "@types/react": "16.9.32",
    "@types/react-dom": "16.9.6",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "@welldone-software/why-did-you-render": "4.0.6",
    "@zeit/next-css": "1.0.1",
    "babel-jest": "25.2.6",
    "babel-loader": "8.1.0",
    "babel-preset-react-app": "9.1.2",
    "contentful": "7.14.2",
    "contentful-management": "5.18.2",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "gray-matter": "4.0.2",
    "husky": "4.2.3",
    "jest": "25.2.7",
    "lint-staged": "10.1.1",
    "next": "9.3.4",
    "prettier": "2.0.2",
    "raw-loader": "4.0.0",
    "regenerator-runtime": "0.13.5",
    "secretlint": "1.0.3",
    "typescript": "3.8.3",
    "webpack": "4.42.1"
  },
  "engines": {
    "node": ">=12"
  }
}
