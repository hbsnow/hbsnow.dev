{
  "name": "hbsnow.dev",
  "version": "1.0.0",
  "main": "index.js",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build && next export && node genarateSitemap.js",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint 'src/**/*.{ts,tsx}' && prettier --check --ignore-path .gitignore '**/*.{js,json,md}' && secretlint '**/*' && textlint 'src/posts/**'",
    "lintfix": "eslint --fix 'src/**/*.{ts,tsx}' && prettier --write --ignore-path .gitignore '**/*.{js,json,md}'",
    "typecheck": "tsc --noEmit",
    "test": "jest --watch --silent=false --verbose false",
    "test:ci": "jest --ci --silent",
    "build:model": "contentful-typescript-codegen --output src/models/contentful.d.ts",
    "yarn-deduplicate": "npx yarn-deduplicate yarn.lock"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "secretlint"
    ],
    "*.{js,json,md}": [
      "prettier --write --ignore-path .gitignore '**/*.{js,json,md}'",
      "textlint 'src/posts/**'",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tsc --noEmit",
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {
    "classnames": "2.2.6",
    "color": "3.1.2",
    "dayjs": "1.8.32",
    "normalize.css": "8.0.1",
    "react": "16.13.1",
